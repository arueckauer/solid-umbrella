{
    "name": "arueckauer/solid-umbrella",
    "type": "project",
    "description": "GHA CI & autamatic release test ",
    "homepage": "https://github.com/arueckauer/solid-umbrella",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Andi RÃ¼ckauer",
            "email": "arueckauer@gmail.com",
            "homepage": "https://arueckauer.github.io/",
            "role": "Project Lead"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "^2.4",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "SolidUmbrella\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SolidUmbrellaTest\\": "test/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
